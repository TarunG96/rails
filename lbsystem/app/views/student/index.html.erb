<body>
	<%= render partial: "admin/header" %>
	<%= render partial: "admin/lframe" %>
	<div name="right_frame" align="center" style="float:left; border:2px solid black; width:881px; height:480px;">
    <h1>Students</h1>
    <table name="book_issue_records" border = "1">
      <tr>
        <th>Department Id</th>
        <th>ID</th>
        <th>Name</th>
      </tr>
      <% @students.each do |record|%>
      <tr>
        <td><%= record.department_id%></td>
        <td><%= record.id%></td>
        <td><%= record.name%></td>
      </tr>
      <% end %>
    </table><br><br>
    <%= link_to "Back", "/back" %> 
  </div>
  <%= render partial: "admin/footer" %>
</body>