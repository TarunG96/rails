<body>
	<%= render partial: "admin/header" %>
	<%= render partial: "admin/lframe" %>
	<div name="right_frame" align="center" style="float:left; border:2px solid black; width:881px; height:480px;">
    <h1>Students</h1>
    <table name="books" border = "1">
      <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>City</th>
        <th>Email</th>
        <th>Department ID</th>
        <th>image</th>
      </tr>
      <tr>
        <td><%= @student.id %></td>
        <td><%= @student.name %></td>
        <td><%= @student.city %></td>
        <td><%= @student.email %></td>
        <td><%= @student.department_id %></td>
        <td><%= image_tag @student.image %></td>
      </tr>
    </table><br><br>
    <%= link_to 'Back', {:action => 'index'} %>
  </div>
  <%= render partial: "admin/footer" %>
</body>